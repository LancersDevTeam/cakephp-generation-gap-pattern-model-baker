{
    "name": "your-name-here/generation-gap-model-baker",
    "description": "GenerationGapModelBaker plugin for CakePHP4",
    "type": "cakephp-plugin",
    "keywords": ["cakephp", "bake"],
    "homepage": "https://github.com/LancersDevTeam/cakephp-generation-gap-pattern-model-baker",
    "license": "MIT",
    "require": {
        "php": ">=7.2",
        "cakephp/cakephp": "~4.1.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "GenerationGapModelBaker\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "GenerationGapModelBaker\\Test\\": "tests/",
            "Cake\\Test\\": "vendor/cakephp/cakephp/tests/"
        }
    }
}
